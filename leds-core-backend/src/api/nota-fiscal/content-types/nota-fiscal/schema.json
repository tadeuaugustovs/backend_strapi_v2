{
  "kind": "collectionType",
  "collectionName": "nota_fiscals",
  "info": {
    "singularName": "nota-fiscal",
    "pluralName": "nota-fiscals",
    "displayName": "NotaFiscal",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "data_pagamento": {
      "type": "date"
    },
    "natureza_despesa": {
      "type": "enumeration",
      "enum": [
        "Passagem",
        "Pessoa Jurídica",
        "Hospedagem/Alimentação",
        "Material Permanente"
      ]
    },
    "forma_pagamento": {
      "type": "enumeration",
      "enum": [
        "Pix",
        "Cheque",
        "Cartão Magnético",
        "DOC",
        "TED",
        "Saque",
        "Contrato de Câmbio"
      ]
    },
    "numero_documento": {
      "type": "string"
    },
    "valor_liquido": {
      "type": "decimal"
    },
    "arquivos": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "projetos": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::projeto.projeto",
      "inversedBy": "nota_fiscals"
    },
    "dados_pessoa_juridica": {
      "type": "component",
      "repeatable": false,
      "component": "financeiro.dados-pessoa-juridica"
    },
    "dados_passagem": {
      "type": "component",
      "repeatable": false,
      "component": "financeiro.dados-passagem"
    },
    "dados_material_permanente": {
      "type": "component",
      "repeatable": false,
      "component": "financeiro.dados-material-permanente"
    },
    "dados_hospedagem_alimentacao": {
      "type": "component",
      "repeatable": false,
      "component": "financeiro.hospedagem-alimentacao"
    }
  }
}
